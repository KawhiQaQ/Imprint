# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DATABASE_PATH=./data/travel-planner.db

# File Storage Configuration
STORAGE_PATH=./uploads

# Aliyun OSS Configuration (可选，用于存储图片到云端)
# 如果配置了 OSS，图片将上传到 OSS 并返回公网 URL，支持 AI 图像分析
# 如果不配置，将使用本地存储（但 AI 图像分析功能将不可用）
OSS_REGION=oss-cn-hangzhou
OSS_ACCESS_KEY_ID=your_oss_access_key_id
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS_BUCKET=your_bucket_name
# OSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com  # 可选，默认根据 region 自动生成

# DeepSeek API Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# Qwen-VL API Configuration (通义千问视觉版)
QWEN_VL_API_KEY=your_qwen_vl_api_key_here
QWEN_VL_BASE_URL=https://dashscope.aliyuncs.com/api/v1

# Wanx API Configuration (通义万相)
WANX_API_KEY=your_wanx_api_key_here
WANX_BASE_URL=https://dashscope.aliyuncs.com/api/v1
# 模型版本选择: wanx-v1 (便宜) 或 wanx2.1-t2i-turbo (质量更高)
WANX_MODEL=wanx-v1

# Unsplash API Configuration
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here
UNSPLASH_ENABLED=true

# Amap (高德地图) API Configuration
# 申请地址: https://console.amap.com/dev/key/app
AMAP_API_KEY=your_amap_api_key_here

# Tavily API Configuration
TAVILY_API_KEY=your_tavily_api_key_here
